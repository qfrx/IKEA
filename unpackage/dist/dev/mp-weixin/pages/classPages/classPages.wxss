.classPages {
  position: relative;
  width: 100%;
}
.classPages .classPages-top {
  width: 100%;
  height: 360rpx;
  padding: 0 40rpx;
  padding-top: 40rpx;
  background: url(data:image/jpeg;base64,) no-repeat center top;
  background-size: cover;
}
.classPages .classPages-top .classPages-top-search {
  display: flex;
  align-items: center;
  width: 100%;
  height: 70rpx;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
  padding-left: 30rpx;
  border: 2rpx solid #ccc;
  border-radius: 999px;
  font-size: 28rpx;
  color: #fff;
  background-color: rgba(0, 0, 0, 0.2);
}
.classPages .classPages-top .classPages-top-search .iconfont {
  font-weight: 700;
  margin-right: 30rpx;
}
.classPages .classPages-top .classPages-top-title {
  width: 100%;
  font-size: 52rpx;
  color: #fff;
  font-weight: 700;
}
.classPages .classPages-box {
  position: absolute;
  left: 0;
  top: 326rpx;
  display: flex;
  width: 100%;
  height: calc(100vh - 360rpx);
  border-top-left-radius: 20px;
  border-top-right-radius: 20px;
  overflow: hidden;
  background-color: #fff;
}
.classPages .classPages-box .classPages-box-left {
  width: 160rpx;
  height: 100%;
}
.classPages .classPages-box .classPages-box-left .scroll-box {
  width: 100%;
  height: 100%;
}
.classPages .classPages-box .classPages-box-left .scroll-box .scroll-list {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 96rpx;
  padding: 0 20rpx;
  font-size: 24rpx;
  font-weight: 600;
  text-align: center;
  color: #676767;
  background-color: #fff;
}
.classPages .classPages-box .classPages-box-left .scroll-box .scroll-list.active {
  color: #055ca5;
}
.classPages .classPages-box .classPages-box-right {
  width: calc(100% - 160rpx);
  height: 100%;
  background-color: #fff;
}
.classPages .classPages-box .classPages-box-right .scrollY-box {
  width: 100%;
  height: 100%;
}
.classPages .classPages-box .classPages-box-right .scrollY-box .scrollY-box-title {
  width: 100%;
  height: 160rpx;
  line-height: 160rpx;
  font-size: 24rpx;
  text-align: center;
}
.classPages .classPages-box .classPages-box-right .scrollY-box .scrollY-box-ul {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  width: 100%;
  padding: 0 56rpx;
  margin: 0;
}
.classPages .classPages-box .classPages-box-right .scrollY-box .scrollY-box-ul .scrollY-box-li {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 200rpx;
  padding: 24rpx;
}
.classPages .classPages-box .classPages-box-right .scrollY-box .scrollY-box-ul .scrollY-box-li .scrollY-box-li-img {
  width: 100rpx;
  height: 110rpx;
  margin-bottom: 12rpx;
  display: block;
}
.classPages .classPages-box .classPages-box-right .scrollY-box .scrollY-box-ul .scrollY-box-li .scrollY-box-li-title {
  font-size: 24rpx;
  text-align: center;
}
view {
  box-sizing: border-box;
}

